<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CropLayout">
        <attr name="cropme_max_scale" format="integer" />
    </declare-styleable>
    <declare-styleable name="SquareCropOverlay">
        <attr name="cropme_result_width" format="fraction" />
        <attr name="cropme_result_height" format="fraction" />
        <attr name="cropme_with_border" format="boolean" />
        <attr name="cropme_background_alpha" format="fraction" />
    </declare-styleable>
    <declare-styleable name="CircleCropOverlay">
        <attr name="crop_result_width" format="fraction" />
        <attr name="crop_result_height" format="fraction" />
        <attr name="crop_with_border" format="boolean" />
        <attr name="crop_background_alpha" format="fraction" />
    </declare-styleable>
</resources>
